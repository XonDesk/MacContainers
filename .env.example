# Timezone
TZ=America/Los_Angeles

# Z-Wave JS UI
# Generate a random session secret for the web UI
ZWAVE_SESSION_SECRET=unique_random_session_secret_here

# Matter Hub
# Long-lived access token from Home Assistant
HAMH_ACCESS_TOKEN=your_home_assistant_access_token_here

# GitHub Runner
# URL of the repository to run actions for
RUNNER_REPO_URL=https://github.com/your-org/your-repo
# Runner registration token (get this from GitHub Repo Settings -> Actions -> Runners -> New self-hosted runner)
RUNNER_REGISTRATION_TOKEN=your_runner_registration_token_here

# Haus of Sutra Instagram Sync
HAUSOFSUTRA_GIT_NAME=Hausofsutra Bot
HAUSOFSUTRA_GIT_EMAIL=bot@hausofsutra.local
HAUSOFSUTRA_REPO=git@github.com:your-org/your-repo.git
